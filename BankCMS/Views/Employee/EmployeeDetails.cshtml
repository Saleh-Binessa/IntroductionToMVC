@model BankCMS_.Models.Employee

<h2>Employee Details:</h2>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID
        </dt>
        <dd class="col-sm-10">
            @Model.EmployeeId
        </dd>
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Model.FirstName @Model.LastName
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.Email
        </dd>
        <dt class="col-sm-2">
            Active
        </dt>
        <dd class="col-sm-10">
            @Model.Department
        </dd>
    </dl>
</div>
<a href="@Url.Action("Index", "Employee")" class="btn btn-info">Back to List</a>